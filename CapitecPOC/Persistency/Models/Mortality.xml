    <Formula>
      <Name>AccMortRate</Name>
      <Parent>Accidental Mortality</Parent>
      <Previous>SeasonalAccMrtAdj</Previous>
      <LastModifiedBy>guy</LastModifiedBy>
      <LastModifiedOn>2025-10-31T09:51:43.9722065+00:00</LastModifiedOn>
      <CreatedBy>guy</CreatedBy>
      <CreatedOn>2025-10-31T09:51:43.9722065+00:00</CreatedOn>
      <Folder>false</Folder>
      <LastModified>2025-10-31T09:51:43.9722065+00:00</LastModified>
      <Notes>Probability of life dying during month t</Notes>
      <OpeningInformation />
      <IsExpanded>true</IsExpanded>
      <DataType>Double</DataType>
      <Value>Dim SelectLU As String
Dim SelectBand As String
Dim SelectMortLU As String
Dim MrtRFScl As Double
Dim AccMrtRate As Double

'Assume zero mortality until the later of valuation date and policy commencement date
If t &lt;= Max(PeriodIF(), 0) Then Return 0

SelectLU = String.Concat("Acc",SEX, SMK)
SelectMortLU = String.Concat("Acc",SEX, SMK, AgeLast(t).ToString())

If PeriodIF() &lt;= Tbl_MrtSelBands.Value(SelectLU, 1) Then 
    SelectBand = "1"
Else If PeriodIF() &lt;= Tbl_MrtSelBands.Value(SelectLU, 2) Then 
    SelectBand = "2"
Else 
    SelectBand = "ultimate"
End If

MrtRFScl = Tbl_MrtRFRskScl.Value(RF_RSK) / 100
AccMrtRate = Math.Min(Tbl_Mort.Value(SelectMortLU, SelectBand), 1)

Return Math.Min(MrtRFScl * AccMrtRate, 1)</Value>
      <Format>0.000000</Format>
      <Override>false</Override>
      <IsPrivate>false</IsPrivate>
      <IsUnitTest>false</IsUnitTest>
    </Formula>
